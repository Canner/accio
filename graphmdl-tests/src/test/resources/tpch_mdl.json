        },
        {
          "name": "orders",
          "type": "Orders",
          "relationship": "OrdersCustomer"
        }
      ],
      "primaryKey": "custkey"
    },
    {
      "name": "Lineitem",
      "refSql": "select * from \"canner-cml\".tpch_tiny.lineitem",
      "columns": [
        {
          "name": "orderkey",
          "expression": "l_orderkey",
          "type": "integer"
        },
        {
